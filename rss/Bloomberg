判&lt;/a&gt;&lt;/p&gt;&lt;/li&gt;&lt;/ul&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style="border-collapse: collapse; padding: 0px 10px;"&gt;&lt;table border="0" cellpadding="0" cellspacing="0" width="100%"&gt;&lt;tr&gt;&lt;td style="border-collapse: collapse;"&gt;&lt;h2&gt;最后一件事&lt;/h2&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style="border-collapse: collapse; padding: 16px 0px;"&gt;&lt;table border="0" cellpadding="0" cellspacing="0" width="100%"&gt;&lt;tr&gt;&lt;td style="border-collapse: collapse; border-radius: 8px; padding-bottom: 18px;"&gt;&lt;table border="0" cellpadding="0" cellspacing="0" width="100%"&gt;&lt;tr&gt;&lt;td style="border-collapse: collapse;" valign="top"&gt; &lt;a href="https://links.message.bloomberg.com/s/c/hcxAinQ1JpnzhQUP1DdR8A6xi31-JAJyxcQ5Mr_-C9JXJFk4Ad6c-e_8auWRoYzcba4Bs8RMKmgKODg5ToAApTQMCqp9bqadjxA3VVOvvA0XgJoF3Z4pEFWdGngl6VWYDOaZwgNUuuPFqJJC5zfomDuT8t7mvIcouCcB6Q6LLqg9vtDxeJLgpL9ci3gEfviJ8R6j_8sMQRZ9cb5SQ6O1Q1xR6KwXS7r8p0UEWMdKJ3odveDBCmy7L1LD3lfNY-a7C1AgVjd97qgz3_c69psDQelCHfgG1EeJeyFQ_zHNTNhdmJMVVfGtT9vrsF5p-adwtx1gRbIQfnpxL-kT42X2v2FW5lN4N6P--oCkmqKOiupmesQB5rDlYbcNP5c/TOLDTded_MAK4VasNV8-O4kNS-onsfMj/16" rel="nofollow" style="color: #000000; text-decoration: none; display: block;" target="_blank"&gt;&lt;!--[if mso]&gt;&lt;table width="550"&gt;&lt;tr&gt;&lt;td&gt;&lt;img alt="" src="https://assets.bwbx.io/images/users/iqjWHBFdfxIU/i3jvmtmPwe6A/v0/550x381.jpg" width="550"/&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt; &lt;div style="display:none"&gt;&lt;![endif]--&gt;&lt;img class="email-ssl-image" height="381" src="https://assets.bwbx.io/images/users/iqjWHBFdfxIU/i3jvmtmPwe6A/v0/550x381.jpg" style="border-radius: 8px 8px 0px 0px; width: 550px; height: 381px;" width="550" /&gt;&lt;!--[if mso]&gt;&lt;/div&gt;&lt;![endif]--&gt;&lt;/a&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td valign="top"&gt; &lt;a href="https://links.message.bloomberg.com/s/c/U9Hp4hq3LfCPkVT2ybTPEwMTPWb