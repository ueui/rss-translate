        &lt;![endif]--&gt;&lt;!--[if !mso]&gt;&lt;!--&gt;&lt;!--&lt;![endif]--&gt;&lt;!-- darkmode --&gt;&lt;div style="display: none; font-size: 1px; color: #ffffff; line-height: 1px; overflow: hidden;"&gt;卡内基政治：最新亮点&lt;/div&gt;&lt;div&gt;&lt;table align="center" border="0" cellpadding="0" cellspacing="0"&gt;&lt;!-- product header --&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td&gt;&lt;!--[if mso | IE]&gt;&lt;table align="center" border="0" cellpadding="0" cellspacing="0" class="" role="presentation" style="width:600px;" width="600" bgcolor="#FFFFFF" &gt;&lt;tr&gt;&lt;td style="line-height:0px;font-size:0px;mso-line-height-rule:exactly;"&gt;&lt;![endif]--&gt;&lt;div style="background-color: #FFFFFF; margin: 0px auto;"&gt;&lt;table align="center" border="0" cellpadding="0" cellspacing="0" style="background-color: #FFFFFF; width: 100%;"&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td style="direction: ltr; font-size: 0px; padding: 0; text-align: center;"&gt;&lt;!--[if mso | IE]&gt;&lt;table role="presentation" border="0" cellpadding="0" cellspacing="0"&gt;&lt;tr&gt;&lt;td class="" style="vertical-align:top;width:600px;" &gt;&lt;![endif]--&gt;&lt;div class="mj-column-per-100 mj-outlook-group-fix" style="font-size: 0px; text-align: left; direction: ltr; display: inline-block; vertical-align: top; width: 100%;"&gt;&lt;table border="0" cellpadding="0" cellspacing="0" style="vertical-align: top;" width="100%"&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td align="center" class="light_img" style="font-size: 0px; padding: 0;"&gt;&lt;table border="0" cellpadding="0" cellspacing="0" style="border-collapse: collapse; border-spacing: 0px;"&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td style="width: 600px;"&gt; &lt;a href="https://go.carnegieendowment.org/ODEzLVhZVS00MjIAAAGfveA_VD3HCJgqS_PBjgqXFFNUySrse5Z29OfIDtE5WRbS08f1qOwZJLShnAQIaGbRBke7NvU=" target="_blank"&gt;&lt;img alt="政治 - 卡内基国际和平基金会" height="auto" src="https://ceipimg.s3.amazonaws.com/email/2024/politika-header2024-lm.png" style="border: 0; display: block; text-decoration