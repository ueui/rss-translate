                      &lt;/th&gt; --&gt;&lt;th&gt;&lt;table&gt;&lt;tbody&gt;&lt;tr&gt;&lt;th style="padding-right: 5px;"&gt; &lt;a href="https://go.carnegieendowment.org/ODEzLVhZVS00MjIAAAGbp8_KxOpC4bsz0srtB0fv-IS_maXGnW0e3_543VtEUVqJXjhM2I39537bI53m_-BSIdu2RPA="&gt;&lt;img alt="Facebook" src="https://ceipimg.s3.amazonaws.com/email/2024/ceip-share2024_facebook.png" width="30" /&gt;&lt;/a&gt;&lt;/th&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;/th&gt;&lt;th&gt;&lt;table&gt;&lt;tbody&gt;&lt;tr&gt;&lt;th style="padding-right: 5px;"&gt; &lt;a href="https://go.carnegieendowment.org/ODEzLVhZVS00MjIAAAGbp8_KxBpLSH4foJbpl34000snao-ge734ssaKjbR1q7IcM1Mci5VQvpJSMGf0lgdCWT_OdVA="&gt;&lt;img alt="电报" src="https://ceipimg.s3.amazonaws.com/email/2024/ceip-share2024_telegram.png" width="30" /&gt;&lt;/a&gt; &lt;/th&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;/th&gt;&lt;th&gt;&lt;table&gt;&lt;tbody&gt;&lt;tr&gt;&lt;th&gt;&lt;/th&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;/th&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;!--[if mso | IE]&gt;&lt;/tr&gt;&lt;/table&gt;&lt;![endif]--&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;/div&gt;&lt;!--[if mso | IE]&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;&lt;![endif]--&gt;&lt;/td&gt;&lt;/tr&gt;&lt;!-- footer section --&gt;&lt;tr&gt;&lt;td&gt;&lt;!--[if mso | IE]&gt;&lt;table align="center" border="0" cellpadding="0" cellspacing="0" class="" role="presentation" style="width:600px;" width="600" bgcolor="#FFFFFF" &gt;&lt;tr&gt;&lt;td style="line-height:0px;font-size:0px;mso-line-height-rule:exactly;"&gt;&lt;![endif]--&gt;&lt;div style="background-color: #FFFFFF; margin: 0px auto;"&gt;&lt;table align="center" border="0" cellpadding="0" cellspacing="0" style="background-color: #FFFFFF; width: 100%;"&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td style="direction: ltr; font-size: 0px; padding: 0 10px 50px 10px; text-align: center;"&gt;&lt;!--[if mso | IE]&gt;&lt;table role="presentation" border="0" c