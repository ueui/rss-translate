                      &lt;/th&gt; --&gt;&lt;th&gt;&lt;table&gt;&lt;tbody&gt;&lt;tr&gt;&lt;th style="padding-right: 5px;"&gt; &lt;a href="https://go.carnegieendowment.org/ODEzLVhZVS00MjIAAAGdNTviy1jS435-u_gAfmCY6E27NAvmuj_qCoBOJedgR5J0mDTdCqwvFberJykxJrVTzJJLGkg="&gt;&lt;img alt="YouTube" src="https://ceipimg.s3.amazonaws.com/email/2024/ceip-share2024_youtube.png" width="30" /&gt;&lt;/a&gt; &lt;/th&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;/th&gt;&lt;!-- &lt;th&gt;
                        &lt;table&gt;
