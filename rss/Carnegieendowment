"https://go.carnegieendowment.org/ODEzLVhZVS00MjIAAAGfYonR_8ntxCC6iovKWxVFBh9zrOvpUuCOjwab0YbWQYFQkhnNwXlHsl4weBUBOQzOJYkoAOw="
&gt;&lt;img width="30" src="https://ceipimg.s3.amazonaws.com/email/2024/ceip-share2024_youtube.png" alt="Youtube"&gt;&lt;/a&gt;&lt;/th&gt;
                            &lt;/tr&gt;
                          &lt;/tbody&gt;
                        &lt;/table&gt;
                      &lt;/th&gt;
                      &lt;th&gt;
                        &lt;table&gt;
                          &lt;tbody&gt;
                            &lt;tr&gt;
                              &lt;th style="padding-right:5px"&gt;&lt;a href=
"https://go.carnegieendowment.org/ODEzLVhZVS00MjIAAAGfYonR_0rhY8ZbXWcuuYgRAIXoi2yKTpdUoDfR6xsIqCOR8aCB_2QIbAi_8aWGjjsHWOVSy-0="
&gt;&lt;img width="30" src="https://ceipimg.s3.amazonaws.com/email/2024/ceip-share2024_instagram.png" alt="Instagram"&gt;&lt;/a&gt;&lt;/th&gt;
